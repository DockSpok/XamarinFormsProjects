             x:Class="AgendaNoite.Principal">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Agenda Super Plus+" HorizontalTextAlignment="Center" FontSize="Medium"/>
            <Image Source="" />
            <Button x:Name="btCadastrar" Text="Cadastrar" Clicked="Cadastrar"/>
            <Button x:Name="btListar" Text="Listar"  Clicked= "Listar"/>
            <Button x:Name="btLocalizar" Text="Localizar" Clicked="Localizar"/>
